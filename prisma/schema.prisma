// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Present {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime  @default(now())
  personType  String?
  email       String?
  name        String?
  account     String?
  message     String?
}